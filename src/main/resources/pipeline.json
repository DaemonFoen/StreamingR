{
  "types": {
    "ImageMeta": {
      "javaType": "com.nsu.data.ImageMeta"
    },
    "JpgImage": { "extends": "ImageMeta",
      "javaType": "com.nsu.data.JpgImage"
    }
  },
  "operators": {
    "source": {
      "kind": "source",
      "outputs": {
        "out": {
          "type": "ImageMeta",
          "to": "denoise.in"
        }
      }
    },
    "denoise": {
      "kind": "map",
      "inputs": {
        "in": {
          "type": "ImageMeta",
          "from": "source.out"
        }
      },
      "outputs": {
        "out": {
          "type": "ImageMeta",
          "to": "sink.in"
        }
      }
    },
    "sink": {
      "kind": "sink",
      "inputs": {
        "in": {
          "type": "JpgImage",
          "from": "denoise.out"
        }
      }
    }
  }
}
